{
  "expo": {
    "name": "LinajeApp",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mainapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#38B6FF"
    },
    "ios": {
      "supportsTablet": false,  
      "bundleIdentifier": "com.linajeapp.app",
      "requireFullScreen": true,
      "infoPlist": {
        "UIRequiredDeviceCapabilities": ["arm64"] 
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#38B6FF"
      },
      "package": "com.linajeapp.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "2.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "948f0fd4-e263-41f2-b573-ebf4a68aabf0"
      }
    },
    "owner": "linajeapp",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/948f0fd4-e263-41f2-b573-ebf4a68aabf0"
    }
  }
}
